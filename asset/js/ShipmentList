<script type="text/javascript">

    $(function () {

        $("#restart-application").on('click', function (e) {
            e.preventDefault();
            $.throbber.show({
                message: "Restarting the application...",
                callback: function () {
                    setLocation($(e.target).data("url"));
                    
                }
            });
            return false;
        });

        // add mainmenu tooltips;
        $('#tools [rel=tooltip]').tooltip({ 
            placement: 'bottom'
        });

    });
          
</script>


